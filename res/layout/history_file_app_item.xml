            <TextView android:textSize="@dimen/common_subtitle_text_size" android:textColor="#ff757575" android:gravity="start" android:layout_gravity="start|center" android:id="@id/child_item_size" android:layout_width="wrap_content" android:layout_height="wrap_content" android:singleLine="true" />
        </LinearLayout>
        <FrameLayout android:layout_gravity="end|center" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/share_session_common_list_child_item_operation_area_margin_horizontal" android:layout_marginRight="@dimen/share_session_common_list_child_item_operation_area_margin_horizontal" android:layout_marginStart="@dimen/share_session_common_list_child_item_operation_area_margin_horizontal" android:layout_marginEnd="@dimen/share_session_common_list_child_item_operation_area_margin_horizontal">
