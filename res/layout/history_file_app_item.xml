<?xml version="1.0" encoding="utf-8"?>
<FrameLayout android:descendantFocusability="blocksDescendants" android:layout_width="fill_parent" android:layout_height="@dimen/history_file_list_item_height"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout android:orientation="horizontal" android:id="@id/app_item" android:background="@drawable/history_file_item_bg" android:paddingTop="@dimen/history_file_list_item_padding_top" android:paddingBottom="@dimen/history_file_list_item_padding_top" android:layout_width="fill_parent" android:layout_height="@dimen/history_file_list_item_height">
        <ImageView android:layout_gravity="start|center" android:id="@id/child_item_icon" android:layout_width="@dimen/history_file_list_item_icon_width" android:layout_height="@dimen/history_file_list_item_icon_width" android:layout_marginLeft="@dimen/history_file_list_item_padding_left" android:layout_marginRight="@dimen/history_file_list_item_padding_left" android:scaleType="centerCrop" android:layout_marginStart="@dimen/history_file_list_item_padding_left" android:layout_marginEnd="@dimen/history_file_list_item_padding_left" />
        <LinearLayout android:gravity="start|center" android:orientation="vertical" android:layout_width="0.0dip" android:layout_height="fill_parent" android:layout_marginRight="@dimen/history_file_list_text_margin_right" android:layout_weight="1.0" android:layout_marginEnd="@dimen/history_file_list_text_margin_right">
