        </FrameLayout>
    </LinearLayout>
    <ImageView android:layout_gravity="bottom" android:id="@id/bottom_line" android:background="#ffe7e7e7" android:layout_width="fill_parent" android:layout_height="0.5dip" android:layout_marginLeft="@dimen/history_file_line_marginlfet" android:layout_marginStart="@dimen/history_file_line_marginlfet" />
</FrameLayout>