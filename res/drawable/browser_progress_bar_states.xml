    <item android:id="@android:id/background">
        <shape>
            <gradient android:startColor="#ffffffff" android:endColor="#ffffffff" android:centerColor="#ffffffff" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <gradient android:startColor="#ff00daff" android:endColor="#ff00daff" android:centerColor="#ff00daff" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
